<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="njnu.edu.back.dao.main.TideStationMapper">
    <select id="QueryHeightByName" resultType="java.util.HashMap">
        SELECT *
        FROM public.tide_station
        where public.tide_station.name = #{name} ;
    </select>
</mapper>